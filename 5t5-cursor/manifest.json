{
  "name": "5t5-cursor",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "description",
  "author": "beishen",
  "icons": {
    "16": "1.png",
    "48": "1.png",
    "128": "1.png"
  },
  "background": {
    "page": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "default.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "index.js","default.css","left.css","right.css","top.css","bottom.css","1.ico","2.ico","3.ico","4.ico","5.ico"
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}